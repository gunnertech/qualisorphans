<%= simple_form_for(@post) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tweet_id_str %>
    <%= f.input :body %>
    <%= f.input :photo_url %>
    <%= f.input :tweet_created_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
